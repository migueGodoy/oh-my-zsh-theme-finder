{
  "name": "oh-my-zsh-theme-finder",
  "version": "1.0.0",
  "description": "Oh my zsh theme finder",
  "main": "index.js",
  "scripts": {
    "dev:api": "wrangler dev api/index.ts",
    "publish:api": "wrangler publish api/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/migueGodoy/oh-my-zsh-theme-finder.git"
  },
  "keywords": [
    "oh-my-zsh"
  ],
  "author": "Migue Godoy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/migueGodoy/oh-my-zsh-theme-finder/issues"
  },
  "homepage": "https://github.com/migueGodoy/oh-my-zsh-theme-finder#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230214.0",
    "prettier": "2.8.4",
    "standard": "17.0.0",
    "typescript": "4.9.5",
    "vitest": "0.28.5",
    "wrangler": "2.10.0"
  },
  "dependencies": {
    "hono": "2.7.8"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:astro/recommended"
    ],
    "rules": {
      "no-tabs": "off",
      "indent": "off",
      "no-unused-vars": "off",
      "space-before-function-paren": "off"
    },
    "overrides": [
      {
        "files": [
          "*.astro"
        ],
        "parser": "astro-eslint-parser",
        "rules": {
          "astro/no-set-html-directive": "error"
        }
      }
    ]
  }
}
